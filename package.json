{
  "name": "file-distributor",
  "version": "2.13.1",
  "private": true,
  "description": "A GitHub App for distributing files/templates to configured repositories in an organisation.",
  "author": "pleo-oss",
  "license": "ISC",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "swc src -d lib -s --copy-files",
    "build:run": "yarn build && yarn start",
    "start": "node ./lib/start.js",
    "test": "jest",
    "lint": "prettier --check src/**/*.ts test/**/*.ts && eslint --ext .ts src && eslint --ext .ts test",
    "format": "prettier --write src/**/*.ts test/**/*.ts && eslint --fix --ext .ts src && eslint --fix --ext .ts test"
  },
  "dependencies": {
    "ajv": "8.11.0",
    "ajv-merge-patch": "^5.0.1",
    "codeowners-utils": "1.0.2",
    "dotenv": "16.0.3",
    "fp-ts": "^2.13.1",
    "genson-js": "0.0.8",
    "jszip": "3.10.1",
    "mustache": "4.2.0",
    "node-stream-zip": "1.15.0",
    "probot": "12.2.8",
    "yaml": "2.1.3"
  },
  "devDependencies": {
    "@jest/globals": "29.1.2",
    "@octokit/webhooks-types": "6.4.0",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.10",
    "@swc/jest": "0.2.23",
    "@types/ajv-merge-patch": "^4.1.2",
    "@types/jest": "29.1.2",
    "@types/mustache": "4.2.1",
    "@types/node": "18.8.2",
    "@types/yauzl": "2.10.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.1.2",
    "nock": "13.2.9",
    "pino": "8.6.1",
    "prettier": "2.7.1",
    "smee-client": "1.2.3",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
